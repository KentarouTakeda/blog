<div class="container">

  <dib class="post-header">
    <div class="post-title">

      <% if (page.subtitle) { %>
      <h1>
        <%= page.title %><br>
        <%= page.subtitle %><br>
      </h1>
      <% } else { %>
      <h1><%= page.title %></h1>
      <% }  %>

    </div>

    <hr>

    <div class="post-date">
      <%- partial('_partial/date', { datetime: page.date }) %>

    </div>
  </dib>


  <div class="main card">
    <%- partial('_partial/post', { post: page }) %>
  </div>
  <div class="sidebar">
    <%- partial('_partial/sidebar') %>
  </div>
</div>
